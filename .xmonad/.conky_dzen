background yes
out_to_console yes
out_to_x no
# Update interval in seconds
update_interval 1
use_xft yes
# xft font when Xft is enabled
xftfont Courier:5

TEXT
${battery}    ^fg(\#00aa4a)Net:${downspeed eth1} ^fg(\#ff3333)${upspeed eth1} ^fg(\#FFFFFF)Mem:${memperc}% ^fg(\#ebac54)Vol:^fg(\#00ffff)${exec amixer get Headphone | egrep -o "[0-9]+%" | head -1 | egrep -o "[0-9]*"}% ^fg(\#ebac54)  ^fg(\#FFFFFF) ${time %d/%m/%Y} ^fg(\#ebac54) ${time %R}